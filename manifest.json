{
    "manifest_version":2,
    "name":"No rick-roll",
    "version":"1.0.0",
    "description": "This extension warns you every time you click on a link that could involve a rick-roll.",
    "permissions": ["https://youtube.com/*"],
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"],
            "run_at": "document_start"
            
        }
    ]
}